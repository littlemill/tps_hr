      </Label>
      <Button fx:id="backBtn" layoutX="435.0" layoutY="345.0" mnemonicParsing="false" onAction="#setOnActionBackBtn" prefHeight="27.0" prefWidth="116.0" text="Back" />
      <Label layoutX="52.0" layoutY="273.0" text="Used" textFill="#d05b5b">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Label fx:id="usedLbl" layoutX="74.0" layoutY="302.0" textAlignment="CENTER" textFill="#d05b5b">
