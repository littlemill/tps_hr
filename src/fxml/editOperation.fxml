<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox alignment="TOP_CENTER" prefHeight="354.0" prefWidth="600.0" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="600.0" spacing="20.0">
               <children>
                  <Label text="Project Code :">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="codeTfield" prefHeight="25.0" prefWidth="110.0" />
                  <Button fx:id="searchBtn" mnemonicParsing="false" prefHeight="25.0" prefWidth="82.0" text="Search" />
               </children>
            </HBox>
            <Label fx:id="label" prefHeight="170.0" prefWidth="450.0" />
            <VBox prefHeight="77.0" prefWidth="560.0" spacing="12.0">
               <children>
                  <CheckBox fx:id="implementCheck" mnemonicParsing="false" text="This project has been implemented.">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </CheckBox>
                  <HBox prefHeight="0.0" prefWidth="600.0" spacing="20.0">
                     <children>
                        <Label text="Travel Expense (à¸¿) :">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                        <TextField fx:id="expenseTfiled" prefHeight="25.0" prefWidth="110.0" />
                        <Button fx:id="addBtn" mnemonicParsing="false" prefHeight="25.0" prefWidth="74.0" text="Add" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets left="19.0" />
               </padding>
            </VBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="30.0" />
         </padding>
      </VBox>
   </center>
   <bottom>
      <HBox alignment="TOP_RIGHT" prefHeight="77.0" prefWidth="600.0" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="saveBtn" mnemonicParsing="false" prefWidth="90.0" text="Save">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Button fx:id="backBtn" mnemonicParsing="false" prefWidth="90.0" text="Back" />
         </children>
         <padding>
            <Insets right="70.0" top="10.0" />
         </padding>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </HBox>
   </bottom>
</BorderPane>
